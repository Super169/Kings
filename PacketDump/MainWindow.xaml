<Window x:Class="PacketDump.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PacketDump"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnStart" Content="開始" Margin="0,0,85,10" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="btnStart_Click"/>
        <Button x:Name="btnEnd" Content="結束" Margin="0,0,5,10" HorizontalAlignment="Right" Width="75" Height="22" VerticalAlignment="Bottom" Click="btnEnd_Click"/>
        <TextBox x:Name="txtInfo" Margin="10,10,10,37" TextWrapping="Wrap"/>

    </Grid>
</Window>
